    isNarrator: 0
    isPlayerCharacter: 0
  - speakingCharacter: {fileID: 11400000, guid: 0a649c9cba84f8d4da596355a2726c19, type: 2}
    emotion:
      name: Munchies_DefaultSmile
      sprite: {fileID: 21300000, guid: 2156e947ecbb8154da87aeadafa79d2a, type: 3}
    myIndex: 0
    emotions: []
    dialogueLines:
    - Oh yeah, Gavin's reaching a turn in his destiny, and he's takin' the turn too
      wide.
    - But you, you can bring him back.
    - Pull that feller off the cliff's edge and bring peace back to this jungle,
      capiche?
    isNarrator: 0
    isPlayerCharacter: 0
  - speakingCharacter: {fileID: 11400000, guid: 0a649c9cba84f8d4da596355a2726c19, type: 2}
